SYMBOL SECTION
 c CONST INTERNAL answer
 d INTERNAL n
 l INTERNAL answer
DATA SECTION
 a := 42 [INT]
 b := EXTERNAL CALL INeg((a, [INT])) [INT]
 GLOBAL c := b [INT]
 GLOBAL d := 0 [INT]
CODE SECTION
 d := c [INT]
 g := 0 [INT]
 h := d LT g [BOOL]
 IF h EQ TRUE THEN IFSTAT_0_SEQ_0_LEVEL_0 ELSE IFNEXT_0_SEQ_0_LEVEL_0
 LABEL IFSTAT_0_SEQ_0_LEVEL_0
 i := EXTERNAL CALL INeg((d, [INT])) [INT]
 d := i [INT]
 GOTO IFEND_0_LEVEL_0
 LABEL IFNEXT_0_SEQ_0_LEVEL_0
 j := 0 [INT]
 k := d GT j [BOOL]
 IF k EQ TRUE THEN IFSTAT_0_SEQ_1_LEVEL_0 ELSE IFNEXT_0_SEQ_1_LEVEL_0
 LABEL IFSTAT_0_SEQ_1_LEVEL_0
 CALL Display((d -> e)[INT])
 GOTO IFEND_0_LEVEL_0
 LABEL IFNEXT_0_SEQ_1_LEVEL_0
 LABEL IFEND_0_LEVEL_0
END
PROC SECTION
 PROC LABEL Display
  PARAM m := e [INT]
  l := FALSE [BOOL]
  n := 2 [INT]
  o := EXTERNAL CALL Mod((m, [INT]), (n, [INT])) [INT]
  p := 0 [INT]
  q := o NE p [BOOL]
  l := q [BOOL]
  IF l EQ TRUE THEN IFSTAT_1_SEQ_0_LEVEL_0 ELSE IFNEXT_1_SEQ_0_LEVEL_0
  LABEL IFSTAT_1_SEQ_0_LEVEL_0
  r := 1 [INT]
  EXTERNAL CALL WriteInt((r, [INT]))
  s := 1 [INT]
  t := m ISUB s [INT]
  u := 2 [INT]
  v := EXTERNAL CALL Div((t, [INT]), (u, [INT])) [INT]
  m := v [INT]
  GOTO IFEND_1_LEVEL_0
  LABEL IFNEXT_1_SEQ_0_LEVEL_0
  w := 0 [INT]
  x := m EQ w [BOOL]
  IF x EQ TRUE THEN IFSTAT_1_SEQ_1_LEVEL_0 ELSE IFNEXT_1_SEQ_1_LEVEL_0
  LABEL IFSTAT_1_SEQ_1_LEVEL_0
  y := 1 [INT]
  z := EXTERNAL CALL INeg((y, [INT])) [INT]
  EXTERNAL CALL WriteInt((z, [INT]))
  GOTO IFEND_1_LEVEL_0
  LABEL IFNEXT_1_SEQ_1_LEVEL_0
  A := 0 [INT]
  EXTERNAL CALL WriteInt((A, [INT]))
  B := 2 [INT]
  C := EXTERNAL CALL Div((m, [INT]), (B, [INT])) [INT]
  m := C [INT]
  GOTO IFEND_1_LEVEL_0
  LABEL IFNEXT_1_SEQ_2_LEVEL_0
  LABEL IFEND_1_LEVEL_0
 RETURN

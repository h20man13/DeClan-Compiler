SYMBOL SECTION
 b CONST INTERNAL r1
 d CONST INTERNAL r2
 e INTERNAL result
 f EXTERNAL PARAM RSub 0
 g EXTERNAL PARAM RSub 1
 h EXTERNAL RETURN RSub
 j EXTERNAL PARAM WriteReal 0
DATA SECTION
 a := 7.5 [REAL]
 GLOBAL b := a [REAL]
 c := 49.5 [REAL]
 GLOBAL d := c [REAL]
 GLOBAL e := 0.0 [REAL]
CODE SECTION
 CALL RSub((d -> f)[REAL], (b -> g)[REAL])
 EXTERNAL RETURN i := h [REAL]
 e := i [REAL]
 CALL WriteReal((e -> j)[REAL])
END
PROC SECTION

SYMBOL SECTION
 b CONST INTERNAL r1
 d CONST INTERNAL r2
 e INTERNAL result
 f EXTERNAL PARAM RMul 0
 g EXTERNAL PARAM RMul 1
 h EXTERNAL RETURN RMul
 j EXTERNAL PARAM WriteReal 0
DATA SECTION
 a := 21.0 [REAL]
 GLOBAL b := a [REAL]
 c := 2.0 [REAL]
 GLOBAL d := c [REAL]
 GLOBAL e := 0.0 [REAL]
CODE SECTION
 CALL RMul((b -> f)[REAL], (d -> g)[REAL])
 EXTERNAL RETURN i := h [REAL]
 e := i [REAL]
 CALL WriteReal((e -> j)[REAL])
END
PROC SECTION

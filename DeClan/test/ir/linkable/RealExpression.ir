SYMBOL SECTION
 ENTRY b CONST INTERNAL GLOBAL a 
 ENTRY d CONST INTERNAL GLOBAL b 
 ENTRY f EXTERNAL PARAM RNeg 0
 ENTRY g EXTERNAL RETURN RNeg
 ENTRY i EXTERNAL PARAM IntToReal 0
 ENTRY j EXTERNAL RETURN IntToReal
 ENTRY l EXTERNAL PARAM RAdd 0
 ENTRY m EXTERNAL PARAM RAdd 1
 ENTRY n EXTERNAL RETURN RAdd
 ENTRY p CONST INTERNAL GLOBAL c 
 ENTRY q INTERNAL GLOBAL result 
 ENTRY r INTERNAL GLOBAL bAndAResult 
 ENTRY t EXTERNAL PARAM RMul 0
 ENTRY u EXTERNAL PARAM RMul 1
 ENTRY v EXTERNAL RETURN RMul
 ENTRY x EXTERNAL PARAM RSub 0
 ENTRY y EXTERNAL PARAM RSub 1
 ENTRY z EXTERNAL RETURN RSub
 ENTRY C EXTERNAL PARAM WriteString 0
 ENTRY D EXTERNAL PARAM WriteReal 0
DATA SECTION
 DEF a := 0 <INT>
 DEF GLOBAL b := a <INT>
 DEF c := 1.2 <REAL>
 DEF GLOBAL d := c <REAL>
 DEF e := 3.14 <REAL>
 CALL RNeg([e -> f]<REAL>)
 DEF h := (RETURN g) <REAL>
 CALL IntToReal([(GLOBAL b) -> i]<INT>)
 DEF k := (RETURN j) <REAL>
 CALL RAdd([h -> l]<REAL>, [k -> m]<REAL>)
 DEF o := (RETURN n) <REAL>
 DEF GLOBAL p := o <REAL>
BSS SECTION
 DEF GLOBAL q := 0.0 <REAL>
 DEF GLOBAL r := 0.0 <REAL>
CODE SECTION
 CALL IntToReal([(GLOBAL b) -> i]<INT>)
 DEF s := (RETURN j) <REAL>
 CALL RMul([(GLOBAL d) -> t]<REAL>, [s -> u]<REAL>)
 DEF w := (RETURN v) <REAL>
 GLOBAL r := w <REAL>
 CALL RSub([(GLOBAL p) -> x]<REAL>, [(GLOBAL r) -> y]<REAL>)
 DEF A := (RETURN z) <REAL>
 GLOBAL q := A <REAL>
 DEF B := "Result is " <STRING>
 CALL WriteString([B -> C]<STRING>)
 CALL WriteReal([(GLOBAL q) -> D]<REAL>)
 CALL WriteLn()
END
PROC SECTION

SYMBOL SECTION
 ENTRY b CONST INTERNAL r1
 ENTRY d CONST INTERNAL r2
 ENTRY e INTERNAL result
 ENTRY f EXTERNAL PARAM RDivide 0
 ENTRY g EXTERNAL PARAM RDivide 1
 ENTRY h EXTERNAL RETURN RDivide
 ENTRY j EXTERNAL PARAM WriteReal 0
DATA SECTION
 DEF a := 30.0 [REAL]
 DEF GLOBAL b := a [REAL]
 DEF c := 2.0 [REAL]
 DEF GLOBAL d := c [REAL]
BSS SECTION
 DEF GLOBAL e := 0.0 [REAL]
CODE SECTION
 CALL RDivide(((GLOBAL b) -> f)[REAL], ((GLOBAL d) -> g)[REAL])
 DEF i := (RETURN h) [REAL]
 e := i [REAL]
 CALL WriteReal(((GLOBAL e) -> j)[REAL])
END
PROC SECTION

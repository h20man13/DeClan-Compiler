SYMBOL SECTION
 b EXTERNAL PARAM RNeg 0
 c EXTERNAL RETURN RNeg
 e CONST INTERNAL a
 g CONST INTERNAL b
 h INTERNAL result
 i EXTERNAL PARAM RAdd 0
 j EXTERNAL PARAM RAdd 1
 k EXTERNAL RETURN RAdd
 m EXTERNAL PARAM RealToInt 0
 n EXTERNAL RETURN RealToInt
 p EXTERNAL PARAM WriteReal 0
DATA SECTION
 a := 3.14 [REAL]
 CALL RNeg((a -> b)[REAL])
 EXTERNAL RETURN d := c [REAL]
 GLOBAL e := d [REAL]
 f := 0.5 [REAL]
 GLOBAL g := f [REAL]
 GLOBAL h := 0 [INT]
CODE SECTION
 CALL RAdd((e -> i)[REAL], (g -> j)[REAL])
 EXTERNAL RETURN l := k [REAL]
 CALL RealToInt((l -> m)[REAL])
 EXTERNAL RETURN o := n [INT]
 h := o [INT]
 CALL WriteReal((h -> p)[INT])
END
PROC SECTION

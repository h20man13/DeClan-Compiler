SYMBOL SECTION
 ENTRY b CONST INTERNAL v1
 ENTRY d CONST INTERNAL v2
 ENTRY e INTERNAL result
 ENTRY f EXTERNAL PARAM RAdd 0
 ENTRY g EXTERNAL PARAM RAdd 1
 ENTRY h EXTERNAL RETURN RAdd
 ENTRY j EXTERNAL PARAM WriteReal 0
DATA SECTION
 DEF a := 4.5 [REAL]
 DEF GLOBAL b := a [REAL]
 DEF c := 8.5 [REAL]
 DEF GLOBAL d := c [REAL]
BSS SECTION
 DEF GLOBAL e := 0.0 [REAL]
CODE SECTION
 CALL RAdd(((GLOBAL b) -> f)[REAL], ((GLOBAL d) -> g)[REAL])
 DEF i := (RETURN h) [REAL]
 e := i [REAL]
 CALL WriteReal(((GLOBAL e) -> j)[REAL])
END
PROC SECTION

SYMBOL SECTION
 b CONST INTERNAL v1
 d CONST INTERNAL v2
 e INTERNAL result
DATA SECTION
 a := 4.5 [REAL]
 GLOBAL b := a [REAL]
 c := 8.5 [REAL]
 GLOBAL d := c [REAL]
 GLOBAL e := 0.0 [REAL]
CODE SECTION
 f := EXTERNAL CALL RAdd((b, [REAL]), (d, [REAL])) [REAL]
 e := f [REAL]
 EXTERNAL CALL WriteReal((e, [REAL]))
END
PROC SECTION

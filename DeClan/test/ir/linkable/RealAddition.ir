SYMBOL SECTION
 b CONST INTERNAL v1
 d CONST INTERNAL v2
 e INTERNAL result
 f EXTERNAL PARAM RAdd 0
 g EXTERNAL PARAM RAdd 1
 h EXTERNAL RETURN RAdd
 j EXTERNAL PARAM WriteReal 0
DATA SECTION
 a := 4.5 [REAL]
 GLOBAL b := a [REAL]
 c := 8.5 [REAL]
 GLOBAL d := c [REAL]
 GLOBAL e := 0.0 [REAL]
CODE SECTION
 CALL RAdd((b -> f)[REAL], (d -> g)[REAL])
 EXTERNAL RETURN i := h [REAL]
 e := i [REAL]
 CALL WriteReal((e -> j)[REAL])
END
PROC SECTION

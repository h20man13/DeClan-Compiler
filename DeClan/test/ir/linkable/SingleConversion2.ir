SYMBOL SECTION
 b CONST INTERNAL r
 c INTERNAL result
 d EXTERNAL PARAM RealToInt 0
 e EXTERNAL RETURN RealToInt
 g EXTERNAL PARAM WriteInt 0
DATA SECTION
 a := 6.5 [REAL]
 GLOBAL b := a [REAL]
 GLOBAL c := 0 [INT]
CODE SECTION
 CALL RealToInt((b -> d)[REAL])
 EXTERNAL RETURN f := e [INT]
 c := f [INT]
 CALL WriteInt((c -> g)[INT])
END
PROC SECTION

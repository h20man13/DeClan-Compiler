SYMBOL SECTION
 ENTRY b CONST INTERNAL r
 ENTRY c INTERNAL result
 ENTRY d EXTERNAL PARAM RealToInt 0
 ENTRY e EXTERNAL RETURN RealToInt
 ENTRY g EXTERNAL PARAM WriteInt 0
DATA SECTION
 DEF a := 6.5 <REAL>
 DEF GLOBAL b := a <REAL>
BSS SECTION
 DEF GLOBAL c := 0 <INT>
CODE SECTION
 CALL RealToInt([(GLOBAL b) -> d]<REAL>)
 DEF f := (RETURN e) <INT>
 GLOBAL c := f <INT>
 CALL WriteInt([(GLOBAL c) -> g]<INT>)
END
PROC SECTION

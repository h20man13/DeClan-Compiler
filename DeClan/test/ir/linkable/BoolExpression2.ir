SYMBOL SECTION
 ENTRY b CONST INTERNAL a
 ENTRY d CONST INTERNAL e
 ENTRY f CONST INTERNAL x
 ENTRY g INTERNAL result
 ENTRY i EXTERNAL PARAM WriteString 0
 ENTRY j EXTERNAL PARAM WriteBool 0
 ENTRY l EXTERNAL PARAM IntToReal 0
 ENTRY m EXTERNAL RETURN IntToReal
 ENTRY o EXTERNAL PARAM RGreaterThanOrEqualTo 0
 ENTRY p EXTERNAL PARAM RGreaterThanOrEqualTo 1
 ENTRY q EXTERNAL RETURN RGreaterThanOrEqualTo
DATA SECTION
 DEF a := 0 [INT]
 DEF GLOBAL b := a [INT]
 DEF c := TRUE [BOOL]
 DEF GLOBAL d := c [BOOL]
 DEF e := 1.0 [REAL]
 DEF GLOBAL f := e [REAL]
BSS SECTION
 DEF GLOBAL g := FALSE [BOOL]
CODE SECTION
 DEF h := "e is " [STRING]
 CALL WriteString((h -> i)[STRING])
 CALL WriteBool(((GLOBAL d) -> j)[BOOL])
 CALL WriteLn()
 DEF k := BNOT (GLOBAL d) [BOOL]
 CALL IntToReal(((GLOBAL b) -> l)[INT])
 DEF n := (RETURN m) [REAL]
 CALL RGreaterThanOrEqualTo(((GLOBAL f) -> o)[REAL], (n -> p)[REAL])
 DEF r := (RETURN q) [BOOL]
 DEF s := k LOR r [BOOL]
 GLOBAL g := s [BOOL]
 CALL WriteBool(((GLOBAL g) -> j)[BOOL])
END
PROC SECTION
